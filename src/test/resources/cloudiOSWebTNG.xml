<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="iOS Web mobile testing Suite for EPAM Mobile Cloud">
    <parameter name="appType" value="web"/>
    <!--iPhone 12 mini-->
    <parameter name="udid" value="00008101-0000141E3E78001E"/>
    <parameter name="platformName" value="iOS"/>
    <parameter name="browserName" value="Safari"/>
    <parameter name="url" value="http://google.com"/>
    <parameter name="textForSearch" value="EPAM"/>
    <test name="iOS web tests">
        <groups>
            <run>
                <include name="web"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>